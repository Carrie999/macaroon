{
	"name": "Macaroon",
	"displayName": "Macaroon 2022",
	"description": "Macaroon",
	"version": "0.1.0",
	"author": "yajing pang",
	"publisher": "Carrie999",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Carrie999/macaroon"
	},
	"engines": {
		"vscode": "^1.33.0"
	},
	"keywords": [
		"soft",
		"macaroon"
	],
	"categories": [
		"Themes"
	],
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.js",
	"contributes": {
		"themes": [
			{
				"label": "macaroon",
				"uiTheme": "vs-dark",
				"path": "./themes/synthwave-color-theme.json"
			}
		],
		"commands": [
			{
				"command": "macaroon.enableNeon",
				"title": "macaroon"
			},
			{
				"command": "macaroon.disableNeon",
				"title": ""
			},
			{
				"command": "iu: Disable Neon Dreams.whatsNew",
				"title": ""
			}
		],
		"configuration": {
			"title": "Macaroon 2022",
			"properties": {
				"iu.brightness": {
					"type": "number",
					"default": 0.45,
					"description": "Set the brightness value for the glow effect. 0.0 is fully transparent and 1.0 is fully bright"
				},
				"iu.disableGlow": {
					"type": "boolean",
					"default": false,
					"description": "Disable the glow effect"
				}
			}
		}
	},
	"dependencies": {
		"semver": "^7.1.1"
	},
	"__metadata": {
		"publisherDisplayName": "Carrie999"
	}
}